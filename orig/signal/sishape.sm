sph 2    data $1
         read {tp 1}
        # set tp1 = tp-0.1
        # set tp2 = tp+0.1
        # set tap = 50 + 0.00000001*tp
         lweight 4
         do i = 0, dimen(tp)-1 {
           define xpo (tp[$i])
         #  define xpo1 (tp1[$i])
         #  define xpo2 (tp2[$i])
         #  define ypo (tap[$i])
           ctype $2
         #  relocate $xpo1 0
         #  draw $xpo $ypo
         #  draw $xpo2 0
           relocate $xpo 0
           draw $xpo 5
         }
         lweight 2
         ctype black

phs 2    data $1
         read {ts 2 is 3}
         do i = 0, dimen(ts)-1 {
           define x0 (ts[$i])
           define t0 (ts[$i] + 2.95)
           define ni (is[$i])
           hga $x0 $ni $t0 1.4154 0.83725 $2
         }
          

mahi 6
         data $1
         if ($3 == 3) {read rpo 3}
         if ($3 == 4) {read rpo 4}
         if ($3 == 5) {read rpo 5}
         if ($3 == 6) {read rpo 6}
         if ($3 == 7) {read rpo 7}
         if ($3 == 8) {read rpo 8}
         if ($3 == 9) {read rpo 9}
         if ($3 == 10) {read rpo 10}
         if ($3 == 11) {read rpo 11}
         set rp = rpo/$2
         define rpmin  100000
         define rpmax  -100000
         do i = 0, dimen(rp)-1 {
           if (rp[$i] < $rpmin) {define rpmin (rp[$i])}
           if (rp[$i] > $rpmax) {define rpmax (rp[$i])}
         }
         write standard limits $rpmin $rpmax
         set dimen(hr) = 101
         set dimen(pe) = 101
         define np (dimen(rp))
         do i = 0, 100 {
           set pe[$i] = $4 + $i*$5
           set hr[$i] = 0
         }
         do i = 0, $np-1 {
           do j = 0, 100 {
             if (rp[$i] >= pe[$j]-0.5*$5 && rp[$i] < pe[$j]+0.5*$5) {
               set hr[$j] = hr[$j] + 1
             }
           }
         }
         define hrmax 0
         do i = 0, 100 {
           if (hr[$i] > $hrmax) {define hrmax (hr[$i])}
         }
         define hrsum 0
         define hrsuma 0
         do i = 0, 100 {
           set hr[$i] = hr[$i]/$hrmax
           define hrsum ($hrsum + pe[$i]*hr[$i])
           define hrsuma ($hrsuma + hr[$i])
         }
         define prmean ($hrsum/$hrsuma)
